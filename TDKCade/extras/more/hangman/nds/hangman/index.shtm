<FORM name="hangform" >
<STRONG>Previous guesses:</STRONG><BR>
<input type="text" size=26 name="guessed">
</form>

<form><input type="button" name="start" value="New game" onclick="if (ready==0) {alert('Please wait, caching images');} else {setup();}">
</form>

<script>

function keyDivert(keyPressed) 
{
var whichbrowser = (document.layers) ? keyPressed.which : event.keyCode;
var letterpressed = String.fromCharCode(whichbrowser).toLowerCase();
guess(letterpressed);

}

document.onkeypress = keyDivert;
var impatient=0;
var offset=-2;
var ready=0
var score=0;
var word="";
var howdead=0;

// ---------- word list is protected by copyright. Unauthorised reproduction or adaptation prohibited
wordlist=new Array("chair","phlegm","fuchsia","rhythm","buxom","fjord","gnarly","twelve","cycle","whistle","yummy","canary","agnostic","psychotic","zoom","jinx","charity","wonder","yet","jeopardy","machine","clown","segue","inert","jailbait","oblique","tiny","cataclysm", "marvellous","insipid", "archaic", "heinous","bogus","trundle","cirrus","lynx","consider","think","bony","jiffy","do","twice","life","november","february","limited","stamp","authority","phone","earth","tsunami","conifer","weasel","slaughter","drivel","suspect","pillow","hobo","triumph","bystander","camber","democrat","wally","weirdo","turbine","amplify","iguana","xylophone","giraffe","craft","sympathy","jewellery","spaghetti","origami","chopstick","placebo","workhorse","urban","suburb","blatant","psycho","chrysalis","gorgon","errand","worthwhile","ethereal","gown","monkey","betwixt","almighty","inset","angular","ocular","nougat","photon","nautical","nuisance","invoke","nova","boomerang","motif","moribund","mud","nit","zany","paste","zinc","outrun","complex","clientele","nicotine","ochre","mummify","winceyette","copse","windpipe","fortunate","fickle","craving","viaduct","aqueduct","aqueous","glimmer","vitriol","escalator","brawn","brevity","revision","cathode","pendulous","dell","preamble","pitchfork","mitre","comb","twitch","mitten","nirvana","vitality","ceiling","cranberry","celluloid","cavort","caviar","cohort","cinch","gristle","lizard","wriggle","dictaphone","precarious","ecstasy","oxymoron","turquoise","kiwi","wrought","faint","edge","rhetoric","parsimony","spangle","plural","enormity","magnitude","magnet","balance","fey","jocund","breeze","encrust","pyre","gymnast","crystal","obviate","bamboozle","porpoise","relegate","womb","printing","supreme","candle","consumer","question","career","job","final","hangman","navy","gem","audit","expect","preclude","wasp","bespoke");
// ---------- word list is protected by copyright. Unauthorised reproduction or adaptation prohibited

// ---------- set up a game to start

function setup()
{
document.gallows.src="glass.gif";
score=0;
chosen=Math.floor(Math.random()*wordlist.length);
word=wordlist[chosen];
howdead=0;

for (i=0;i<21;i++)
{document.images[i+8+offset].src="glass.gif";}


for (i=0;i<word.length;i++)
{document.images[18+i+offset].src="dash.gif";}

document.hangform.guessed.value="";

}




function guess(guessedletter)
{

if (word.length==0) {return;}


// --------- only proceed if they haven't already guessed that letter

if (document.hangform.guessed.value.indexOf(guessedletter)==-1)

{document.hangform.guessed.value+=guessedletter;


// -------- dealing with wrong guesses

if (word.indexOf(guessedletter)=="-1")
{howdead++; document.gallows.src="stand"+howdead+".gif";
if (howdead==11) 
{
alert("Game Over. You lose"); 
for (i=0;i<word.length;i++)
{document.images[8+i+offset].src=word.charAt(i)+".gif"; }
word="";}

}
else


// -------------- dealing with right guesses

{
for (i=0;i<word.length;i++)
{if (word.charAt(i)==guessedletter)
{document.images[8+i+offset].src=guessedletter+".gif"; score++;}
}

if (score==word.length) {alert("Well done! You won!"); word="";}

}


}

// -------------- telling them that they've already guessed that letter

else
{alert ("You've already guessed that letter! Guess again!");}


}


</script>

<a name="help"> </a>
<h2>About</h2>
<p>Guess the hidden word before the man hangs. Enter letters by touching them with the stylus. At the end, use the 'New game' button to start a new game.
<script language="Javascript">
document.write("<p>There are "+wordlist.length+" words to guess.");
// dictionary tester for (i=0;i<wordlist.length;i++){document.write("<a href=http://www.askoxford.com/concise_oed/"+wordlist[i]+"?view=uk>"+wordlist[i]+"</a><BR>");}
</script>
<p><a href="../contact.shtm">Let me know</a> your favourite difficult Hangman words.
<p>This game is designed to show the gallows and word on the top screen, and the letter list, guesses and new game button on the bottom screen. <a href="#play">Align now</a>.
